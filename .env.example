# required
GOTRUE_URL=http://localhost:9999
GOTRUE_ANON_KEY=your-anon-key-here

# server
GRPC_PORT=50051
METRICS_PORT=9090
SERVER_READ_TIMEOUT=10s
SERVER_WRITE_TIMEOUT=10s
GOTRUE_TIMEOUT=30s
ENVIRONMENT=development
LOG_LEVEL=debug

# tls (optional)
TLS_ENABLED=false
# TLS_CERT_FILE=/path/to/cert.pem
# TLS_KEY_FILE=/path/to/key.pem

# google oauth
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=

# apple oauth
# APPLE_CLIENT_ID=
# APPLE_TEAM_ID=
# APPLE_KEY_ID=
# APPLE_PRIVATE_KEY=

# attestation - locks api to your apps only
ATTESTATION_ENABLED=false
# ATTESTATION_IOS_APP_ID=TEAMID.com.yourcompany.yourapp
# ATTESTATION_IOS_ENV=production
# ATTESTATION_ANDROID_PACKAGE=com.yourcompany.yourapp
# ATTESTATION_ANDROID_PROJECT=your-gcp-project-id
# ATTESTATION_ANDROID_KEY=service-account-json
